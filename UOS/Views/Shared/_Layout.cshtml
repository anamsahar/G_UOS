<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>University Of Sargodha</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/Scripts/jquery")

    <script src="https://unpkg.com/gijgo@1.9.13/js/gijgo.min.js" type="text/javascript"></script>
    <link href="https://unpkg.com/gijgo@1.9.13/css/gijgo.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>*@

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <style>
        .label {
            font-weight: 500;
        }

        #overlay {
            position: fixed;
            top: 0;
            left: 0;
            width: 50%;
            margin: 2% 22% 0 22%;
            height: 90%;
            background-color: #000;
            filter: alpha(opacity=50);
            -moz-opacity: 0.5;
            -khtml-opacity: 0.5;
            opacity: 1;
            padding: 0;
            z-index: 10000;
        }

        #overlayimg {
            position: relative;
            width: 100%;
            height: 100%;
        }

        #close {
            float: right;
            display: inline-block;
            padding: 0px 7px;
            /*background: #ccc;*/
        }
    </style>
</head>
<body class="m-0 p-0 bg-ash">
    <nav class="navbar navbar-expand-lg navbar-light bg-primary py-1 px-4">
        <a class="navbar-brand text-white p-0 m-0" href="../UserDashboard/Dashboard"><img src="../Images/logo1.png" class="logo p-0 m-0" /></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon text-white"></span>
        </button>
        @Html.Partial("Navbar")
        </nav>
        <div class="container-fluid body-content  bg-ash">
            <div class="container bg-white " >
            </div>
            @RenderBody()
        </div>
        @*@Html.Partial("Footer1")*@
        @RenderSection("scripts", required: false)


</body>
</html>
